<%= form_for @patient_appointment, url: patient_appointments_path(doctor_id: @current_doctor, user_id: @current_user) do |f| %>
  <div>
    <%= f.label :start_time %>
    <%= f.text_field :start_time %>
  </div>

  <div>
    <%= f.label :date %>
    <%= f.text_field :date %>
  </div>

  <div>
    <%= f.submit "Confirm Booking" %>
  </div>
<% end %>
